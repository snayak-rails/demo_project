<h1>Signup!</h1>

<%= form_for :user, url: users_path do |f| %>

  Name: <%= f.text_field :name %>
  <br>
  Gender: <%= f.text_field :gender %>
  <br>
  Email: <%= f.text_field :email %>
  <br>
  Contact: <%= f.number_field :contact %>
  <br>
  Role: <%= f.text_field :role %>
  <br>
  Password: <%= f.password_field :password %>
  <br>
  <%= f.submit "Submit" %>

<% end %>